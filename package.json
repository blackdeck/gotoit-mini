  "peerDependencies": {
    "babel-core": "6.x || ^7.0.0-bridge.0"
  },
  "scripts": {
    "watch-sass-theme": "./node_modules/node-sass-chokidar/bin/node-sass-chokidar ./src/assets/styles/scss/theme -o ./src/assets/styles/  --source-map true --source-map-embed",
    "start": "npm run watch-sass-theme -- --watch | node scripts/start.js",
    "build": "npm run watch-sass-theme | node scripts/build.js",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint:js": "eslint ./src --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint './src/**/*.scss'",
    "test": "node scripts/test.js --env=jsdom",
    "desktop-linux": "cross-env BUILD_TARGET=electron-renderer npm run build && electron-builder --linux --x64",
    "desktop-windows": "cross-env BUILD_TARGET=electron-renderer npm run build && electron-builder --win --x64",
    "desktop-mac": "cross-env BUILD_TARGET=electron-renderer npm run build && electron-builder --mac --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "electronVersion": "3.0.0",
    "appId": "go-to-it",
    "extends": null,
    "directories": {
      "buildResources": "./build",
      "output": "./build_app"
    },
    "fileAssociations": [
      {
        "ext": [
          ".mp3",
          ".wmv",
          ".wav",
          ".ogg"
        ],
        "description": "Music file extension",
        "role": "none"
      }
    ],
    "asar": true,
    "files": [
      {
        "from": "./electron",
        "to": "./",
        "filter": [
          "index.js"
        ]
      },
      {
        "from": "./build",
        "to": "./",
        "filter": [
          "index.html",
          "icon.png"
        ]
      },
      {
        "from": "./build/static",
        "to": "./static"
      },
      "build/static",
      "build/index.html"
    ],
    "extraResources": [
      {
        "from": "./electron",
        "to": "./",
        "filter": [
          "electronPreload.js"
        ]
      }
    ],
    "linux": {
      "target": "deb",
      "maintainer": "BlackDeckCrew",
      "category": "Game"
    }
  },
  "optionalDependencies": {
    "7zip-bin-linux": "^1.3.1",
    "app-builder-bin-linux": "^1.8.9"
  },
  "browserslist": "electron 3.0",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
